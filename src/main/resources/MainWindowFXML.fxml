<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="background" stylesheets="@CSS/StylesButton.css"
      GridPane.columnIndex="1" GridPane.rowIndex="1" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SnakeGame.GameSystem.MainWindowController">

   <Button fx:id="startButton" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" text="Start" onAction="#clickStartButton">
      <GridPane.margin>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </GridPane.margin></Button>

   <Pane prefHeight="50.0" prefWidth="200.0"/>

   <Button fx:id="exitButton" alignment="CENTER" prefHeight="50.0" prefWidth="200.0" text="Exit" onAction="#clickExitButton"/>
</VBox>
